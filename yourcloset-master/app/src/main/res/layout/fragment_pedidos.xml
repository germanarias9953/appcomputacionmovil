        tools:layout_editor_absoluteX="50dp"
        tools:layout_editor_absoluteY="317dp" />

    <EditText
        android:id="@+id/editCiudad"
        android:layout_width="335dp"
        android:layout_height="43dp"
        android:ems="10"
