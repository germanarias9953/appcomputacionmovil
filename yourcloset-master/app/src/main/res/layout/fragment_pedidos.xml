        tools:layout_editor_absoluteY="369dp" />

    <EditText
        android:id="@+id/editPaisRegion"
        android:layout_width="335dp"
        android:layout_height="43dp"
        android:ems="10"
        android:inputType="time"
        android:text="Pais/Region"
        tools:ignore="MissingConstraints"
